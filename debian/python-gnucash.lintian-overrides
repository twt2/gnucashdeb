# The Python module needs an rpath, since the shared libraries on which it
# depends are not in a directory controlled by the dynamic library loader
python-gnucash: binary-or-shlib-defines-rpath usr/lib/python*/dist-packages/gnucash/_gnucash_core_c.so /usr/lib/gnucash/gnucash
python-gnucash: binary-or-shlib-defines-rpath usr/lib/python*/dist-packages/gnucash/_gnucash_core_c.so /usr/lib/gnucash
