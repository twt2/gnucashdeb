	-rm -rf .libs _libs
install-gncextrasmalliconDATA: $(gncextrasmallicon_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(gncextrasmallicondir)" || $(MKDIR_P) "$(DESTDIR)$(gncextrasmallicondir)"
	@list='$(gncextrasmallicon_DATA)'; test -n "$(gncextrasmallicondir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(gncextrasmallicondir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(gncextrasmallicondir)" || exit $$?; \
	done

uninstall-gncextrasmalliconDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(gncextrasmallicon_DATA)'; test -n "$(gncextrasmallicondir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(gncextrasmallicondir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(gncextrasmallicondir)" && rm -f $$files
